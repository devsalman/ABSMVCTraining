// Write uTVL feature model here
root IBFeatures {
    group allof {
        Login,
        Account {
            group allof {
                Deposit,
                Withdraw {
                    group allof {
                        opt WithdrawFee {Int fee in [1..10000]; }
                        opt CheckMinimumBalance {Int min in [1..1000000]; }
                    }
                }
            }
        },
        opt ContactPage
    }
}
